<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="abstractLanguage" class="org.springframework.xiaofa.AbstractLanguage">
		<lookup-method name="getBean" bean="chinese"/>
	</bean>

	<bean id="chinese" class="org.springframework.xiaofa.Chinese"/>
	<bean id="language" class="org.springframework.xiaofa.Language"/>
	<bean id="english" class="org.springframework.xiaofa.English"/>

</beans>